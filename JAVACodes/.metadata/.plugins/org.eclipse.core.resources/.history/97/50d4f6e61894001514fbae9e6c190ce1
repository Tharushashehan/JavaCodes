
public class Test3 {

	public static void main(String a[]){
		
			int i=1;
			
		for(int x=1;x<10;x=x+2){
			
			for(int y=10;y>x;y=y-2){
				System.out.print(" ");
					}
			
			for(int y=0;y<x;y++){
				System.out.print(i);
					
					for(int z=i-1;z>1;z--){
						System.out.print(z);	
					}
					
					for(int z=i;z<i;z++){
						System.out.print(z);	
					}
					
			}

			System.out.println(" ");
			
				i++;
		}
	}
}
