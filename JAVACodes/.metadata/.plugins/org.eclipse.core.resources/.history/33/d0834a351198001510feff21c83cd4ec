package LabSheet;

class test07{
	int i;
	int j;
	static int sum;
	test07(){
		i = 10;
		j = 20;
		System.out.println("Inside Constructor:i+j = "+(i+j));
	}
	public int display(){
		int k = 30;
		System.out.println("Inside display Method:i+j = "+(i+j));
		sum = i+j+k;
		return sum;
	}
	public static void main(String args[]){
		test07 d = new test07();
		d.display();
		System.out.println("Inside main Method:= "+ sum);
	}
}