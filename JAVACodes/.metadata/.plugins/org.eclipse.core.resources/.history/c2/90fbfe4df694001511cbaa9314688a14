
import java.io.*;

class Test7 {
public static void main(String args[ ]) {
	int a = 10;
			System.out.printf("%d\n",a); // The out is an object of the PrintStream class
			System.out.print(a); // The out is an object of the PrintStream class
			System.out.println(a); // The out is an object of the PrintStream class
			System.out.format("a=",a);
}
}