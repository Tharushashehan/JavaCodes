package LabSheet;


class Test03{

	public static void main (String[] args){
		int[][] a = new int[3][4];
		for (int i=0; i<a.length; i++) {
			for (int j=0; j<a[i].length; j++){
				a[i][j] = i+j;
				System.out.print(a[i].length);
			}
			}
		System.out.println("");
		for (int i=0; i<a.length; i++) {
			for (int j=0; j<a[i].length; j++) {
				System.out.print(" " + a[i][j]);
			}
			System.out.println("");
		}
	}
}